{
  "name": "@cox-automotive/yaml-changelog",
  "description": "A simple cli tool for creating and adding to a project's yaml formatted changelog",
  "version": "0.0.3",
  "author": "Jeremiah Goist",
  "preferGlobal": true,
  "bin": {
    "yaml-changelog": "./bin/run"
  },
  "bugs": "https://github.com/Cox-Automotive/yaml-changelog/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "handlebars": "^4.1.2",
    "inquirer": "^6.3.1",
    "js-yaml": "^3.13.1",
    "moment": "^2.24.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/inquirer": "^6.0.1",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.0.0",
    "globby": "^8",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Cox-Automotive/yaml-changelog",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cox-Automotive/yaml-changelog.git"
  },
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "yaml-changelog",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib *.tsbuildinfo && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint-fix": "tslint -c tslint.json 'src/**/*.ts' --fix"
  },
  "types": "lib/index.d.ts"
}
